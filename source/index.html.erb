    <meta content="width=device-width, initial-scale=1" name="viewport">
    <meta content="Dita Munneke" name="description">
    <meta content="BdgPixel" name="author">
    <!--Fav-->
    <link rel="shortcut icon" href="bodo/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="bodo/images/favicon.ico" type="image/x-icon">

    <!--styles-->
    <link href="bodo/css/bootstrap.min.css" rel="stylesheet">
    <link href="bodo/css/owl.carousel.css" rel="stylesheet">
    <link href="bodo/css/owl.theme.css" rel="stylesheet">
<!--     <link href="bodo/css/magnific-popup.css" rel="stylesheet"> -->
    <link href="bodo/css/style.css" rel="stylesheet">
    <link href="bodo/css/responsive.css" rel="stylesheet">

    <!--fonts google-->
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,500,700' rel='stylesheet' type='text/css'>

