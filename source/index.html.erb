<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Dita Munneke</title>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <meta content="Dita Munneke" name="description">
    <meta content="BdgPixel" name="author">
    <!--Fav-->
    <link rel="shortcut icon" href="bodo/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="bodo/images/favicon.ico" type="image/x-icon">

    <!--styles-->
    <link href="bodo/css/bootstrap.min.css" rel="stylesheet">
    <link href="bodo/css/owl.carousel.css" rel="stylesheet">
    <link href="bodo/css/owl.theme.css" rel="stylesheet">
<!--     <link href="bodo/css/magnific-popup.css" rel="stylesheet"> -->
    <link href="bodo/css/style.css" rel="stylesheet">
    <link href="bodo/css/responsive.css" rel="stylesheet">

    <!--fonts google-->
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,500,700' rel='stylesheet' type='text/css'>

    <!--[if lt IE 9]>
       <script type="text/javascript" src="js/html5shiv.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <!--PRELOADER-->
    <div id="preloader">
      <div id="status">
        <img alt="logo" src="bodo/images/logo-big-dita.png">
      </div>
    </div>
    <!--/.PRELOADER END-->

    <!--HEADER -->
    <div class="header">
      <div class="for-sticky">
        <!--LOGO-->
        <div class="col-md-2 col-xs-6 logo">
          <a href="index.html"><img alt="logo" class="logo-nav" src="bodo/images/logo-dita.png"></a>
        </div>
        <!--/.LOGO END-->
      </div>
      <div class="menu-wrap">
        <nav class="menu">
          <div class="menu-list">
            <a data-scroll="" href="#home" class="active">
              <span>Home</span>
            </a>
           <a data-scroll="" href="#work">
              <span>Portfolio</span>
            </a>
            <a data-scroll="" href="#about">
              <span>Over Dita</span>
            </a>
            <a data-scroll="" href="#contact">
              <span>Contact</span>
            </a>
          </div>
        </nav>
        <button class="close-button" id="close-button">Close Menu</button>
      </div>
      <button class="menu-button" id="open-button">
        <span></span>
        <span></span>
        <span></span>
      </button><!--/.for-sticky-->
    </div>
    <!--/.HEADER END-->

    <!--CONTENT WRAP-->
    <div class="content-wrap">
      <!--CONTENT-->
      <div class="content">
        <!--HOME-->
        <section id="home">
          <div class="container">
            <div class="row">
              <div class="mouse-icon margin-20">
                <div class="scroll"></div>
              </div>
            </div>
          </div>
        </section>
        <!--/.HOME END-->

        <!--WORK-->
        <section class="grey-bg mar-tm-10" id="work">
          <div class="container">
            <div class="row">
              <div class="col-md-3">
                <h3 class="title-small">
                  <span>Portfolio</span>
                </h3>
                <p class="content-detail">
                  Ut wisi enim ad minim veniam, quis nostrud exerci tation ullamcorper suscipit lobortis nisl ut aliquip ex ea commodo consequat. <br><br>Duis autem vel eum iriure dolor in hendrerit in vulputate velit esse molestie consequat, vel illum dolore eu feugiat nulla facilisis at vero eros.
                </p>
              </div>
              <div class="col-md-9 content-right">
                <!--PORTFOLIO IMAGE-->
                <ul class="portfolio-image">
                  <li class="col-md-6">
                    <% link_to '/acryl-olieverf' do %>
                      <img alt="image" src="bodo/images/cat-acryl.png">
                      <div class="decription-wrap">
                        <div class="image-bg">
                           <p class="desc">Acryl en olieverf</p>
                        </div>
                      </div>
                    <% end %>
                  </li>
                  <li class="col-md-6">
                    <% link_to '/klei-brons' do %>
                      <img alt="image" src="bodo/images/cat-brons.png">
                      <div class="decription-wrap">
                        <div class="image-bg">
                          <p class="desc">
                            Klei en brons
                          </p>
                        </div>
                      </div>
                    <% end %>
                  </li>
                  <li class="col-md-6">
                    <% link_to '/potlood-krijt' do %>
                      <img alt="image" src="bodo/images/cat-krijt.png">
                      <div class="decription-wrap">
                        <div class="image-bg">
                          <p class="desc">
                            Potlood en krijt
                          </p>
                        </div>
                      </div>
                    <% end %>
                  </li>
                  <li class="col-md-6">
                    <% link_to '/overige' do %>
                      <img alt="image" src="bodo/images/cat-overige.png">
                      <div class="decription-wrap">
                        <div class="image-bg">
                          <p class="desc">
                            Overige
                          </p>
                        </div>
                      </div>
                    <% end %>
                  </li>
                </ul>
                <!--/.PORTFOLIO IMAGE END-->
              </div>
            </div>
          </div>
        </section>
        <!--/.WORK END-->

        <!--ABOUT-->
        <section id="about">
          <div class="col-md-6 col-xs-12 no-pad">
            <div class="bg-about"></div>
          </div>
          <div class="col-md-6 col-sm-12 col-xs-12 white-col">
            <div class="row">
              <!--OWL CAROUSEL2-->
              <div class="owl-carousel2">
                <div class="col-md-12">
                  <div class="wrap-about">
                    <div class="w-content">
                      <p class="head-about">
                        Lorem ipsum dolor sit amet, consectetur adipisicing elit. Deleniti perferendis, distinctio maxime suscipit sunt minus, alias autem odio debitis nihil mollitia quaerat rerum labore, nam quam consectetur illo error ab.
                      </p>

                      <h5 class="name">
                        Dita Munneke
                      </h5>
                      <img alt="signature" src="bodo/images/signature-dita.png">
                    </div>
                  </div>
                </div>

                <div class="col-md-12 col-sm-12 col-xs-12 white-col">
                  <div class="row">
                    <div class="col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2 col-xs-12">
                      <div class="wrap-about">
                        <table class="w-content">
                          <tr>
                            <td class="title">Naam </td>
                            <td class="break">:</td>
                            <td> Dita Munneke</td>
                          </tr>
                          <tr>
                            <td class="title">Email </td>
                            <td class="break">:</td>
                            <td> ditamunneke@live.nl</td>
                          </tr>
                          <tr>
                            <td class="title">Adres atelier </td>
                            <td class="break">:</td>
                            <td> Fjord 155 , 8224 DJ, Lelystad</td>
                          </tr>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!--/.OWL CAROUSEL2 END-->
            </div>
          </div>
        </section>
        <!--/.ABOUT END-->

        <!--CONTACT-->
        <section id="contact" class="grey-bg">
          <div class="container">
            <div class="row">
              <div class="col-md-3">
                <h3 class="title-small">
                  <span>Contact</span>
                </h3>
                <p class="content-detail">
                  Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat.
                </p>
              </div>
              <div class="col-md-9 content-right">
                <form action="https://formspree.io/jurjenmunneke@me.com"
                      method="POST">
                  <div class="group">
                    <input required="" type="text" name="naam"><span class="highlight"></span><span class="bar"></span><label>Naam</label>
                  </div>
                  <div class="group">
                    <input required="" type="email" name="_replyto"><span class="highlight"></span><span class="bar"></span><label>Email</label>
                  </div>
                  <div class="group">
                    <textarea required="" name="bericht"></textarea><span class="highlight"></span><span class="bar"></span><label>Bericht</label>
                  </div>
                  <input id="sendMessage" type="submit" value="Verstuur Bericht">
                  <input type="hidden" name="_next" value="/" />
                  <input type="hidden" name="_subject" value="Bericht via ditamunneke.nl" />
                </form>
              </div>
            </div>
          </div>
        </section>
        <!--/.CONTACT END-->

        <!--FOOTER-->
        <footer>
          <div class="footer-bottom">
            <div class="container">
              <div class="row">
                <a href="https://dcrazed.com/"><img src="bodo/images/logo-bottom-dita.png" alt="crafted with love" class="center-block" /></a>
              </div>
            </div>
          </div>
        </footer>
        <!--/.FOOTER-END-->

      <!--/.CONTENT END-->
      </div>
    <!--/.CONTENT-WRAP END-->
    </div>


    <script src="bodo/js/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="bodo/js/jquery.appear.js" type="text/javascript"></script>
    <script src="bodo/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="bodo/js/classie.js" type="text/javascript"></script>
    <script src="bodo/js/owl.carousel.min.js" type="text/javascript"></script>
<!--     <script src="bodo/js/jquery.magnific-popup.min.js" type="text/javascript"></script> -->
    <script src="bodo/js/masonry.pkgd.min.js" type="text/javascript"></script>
    <script src="bodo/js/masonry.js" type="text/javascript"></script>
    <script src="bodo/js/smooth-scroll.min.js" type="text/javascript"></script>
    <script src="bodo/js/typed.js" type="text/javascript"></script>
    <script src="bodo/js/main.js" type="text/javascript"></script>
  </body>
</html>
